{
  "name": "babes-club-website",
  "version": "1.0.1",
  "type": "module",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "// DEVELOPMENT": "",
    "start": "webpack serve --mode development --config webpack/webpack.dev.js --hot",
    "dev:check": "npm run format && npm run lint && npm run type-check",
    "dev": "npm start",
    "dev:full": "npm run dev:check && npm start",
    "// BUILD": "",
    "build": "NODE_ENV=production webpack --config webpack/webpack.prod.js",
    "build:clean": "rm -rf dist webpack/templates && npm run build",
    "clean": "rm -rf dist",
    "// CODE QUALITY": "",
    "lint": "eslint 'src/**/*.{ts,tsx}' --max-warnings 0",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css,md}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,js,jsx,json,css,md}'",
    "// UTILITIES": "",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "optimize-images": "node scripts/generate-optimized-images.js",
    "tree:output": "tree -I 'node_modules|.git|dist|notes|python|shine-venv' -o tree-directory-structure.txt",
    "refresh": "sudo rm -rf node_modules package-lock.json && rm -rf dist && npm install",
    "errors": "npm run build > build_errors.txt 2>&1",
    "// CLEANUP": "",
    "local:cleanup": "find public/ -name '.DS_Store' -delete",
    "s3:cleanup": "node scripts/s3.js cleanup",
    "// S3 SYNC": "",
    "s3:sync:build": "node scripts/s3.js sync:build",
    "s3:sync:videos": "node scripts/s3.js sync:videos",
    "s3:sync:images": "node scripts/s3.js sync:images",
    "s3:sync:assets": "node scripts/s3.js sync:assets",
    "// MIME TYPE FIXES": "",
    "s3:fix-js": "node scripts/s3.js fix-js",
    "s3:fix-css": "node scripts/s3.js fix-css",
    "s3:fix-fonts": "node scripts/s3.js fix-fonts",
    "s3:fix-cache": "npm run s3:fix-js && npm run s3:fix-css && npm run s3:fix-fonts",
    "// CLOUDFRONT": "",
    "cloudfront:invalidate": "node scripts/cloudfront-invalidate.js",
    "// DEPLOY": "",
    "deploy": "node scripts/deploy.mjs",
    "deploy:dry": "DRY_RUN=1 node scripts/deploy.mjs",
    "deploy:confirm": "read -p 'Deploy to production? (y/N): ' REPLY && [[ $REPLY =~ ^[Yy]$ ]] && node scripts/deploy.mjs",
    "deploy:with-assets": "ASSETS=1 node scripts/deploy.mjs",
    "// GITHUB ACTIONS": "",
    "ci:lint": "npm run lint",
    "ci:type-check": "npm run type-check",
    "ci:test": "echo \"No tests yet\"",
    "ci:build": "npm run build",
    "analyze": "ANALYZE=true npm run build",
    "analyze:size": "npm run build && npx webpack-bundle-analyzer dist/stats.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.9",
    "@react-google-maps/api": "^2.20.6",
    "axios": "^1.6.7",
    "clsx": "^2.1.0",
    "framer-motion": "^12.0.6",
    "gsap": "^3.12.7",
    "lodash": "^4.17.21",
    "lucide-react": "^0.330.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.22.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudfront": "^3.864.0",
    "@aws-sdk/client-s3": "^3.864.0",
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.23.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/lodash": "^4.17.16",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^22.17.2",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "autoprefixer": "^10.4.17",
    "babel-loader": "^9.2.1",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "dotenv": "^16.4.1",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "html-webpack-plugin": "^5.6.0",
    "mime-types": "^3.0.1",
    "mini-css-extract-plugin": "^2.8.0",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.0",
    "prettier": "^3.6.2",
    "sharp": "^0.34.3",
    "sitemap": "^8.0.0",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.1",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2",
    "webpack": "^5.90.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^7.4.2",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^5.10.0"
  }
}
