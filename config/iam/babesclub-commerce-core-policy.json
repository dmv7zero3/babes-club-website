{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DynamoDbCommerceAccess",
      "Effect": "Allow",
      "Action": [
        "dynamodb:BatchWriteItem",
        "dynamodb:GetItem",
        "dynamodb:PutItem",
        "dynamodb:Query",
        "dynamodb:UpdateItem",
        "dynamodb:DescribeTable"
      ],
      "Resource": [
        "arn:aws:dynamodb:us-east-1:752567131183:table/babesclub-commerce",
        "arn:aws:dynamodb:us-east-1:752567131183:table/babesclub-commerce/index/*"
      ]
    },
    {
      "Sid": "ReadStripeParameters",
      "Effect": "Allow",
      "Action": ["ssm:GetParameter", "ssm:GetParameters"],
      "Resource": "arn:aws:ssm:us-east-1:752567131183:parameter/marketbrewer/babes-club/stripe/*"
    }
  ]
}
